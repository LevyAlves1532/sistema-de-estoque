<%- include("includes/head"); %>
<%- include("includes/nav", { activeMenu: "account" }); %>

<section class="container-lg pt-5">
  <form>
    <input type="hidden" name="_csrf" value="<%= csrfToken %>">
    <div class="form-group">
      <label for="form-sign-in-email">E-mail:</label>
      <input type="email" class="form-control" id="form-sign-in-email">
    </div>
    <div class="form-group">
      <label for="form-sign-in-password">Nova Senha:</label>
      <input type="password" class="form-control" id="form-sign-in-password">
    </div>
    <button type="submit" class="btn btn-primary">Salvar Alterações</button>
  </form>
</section>

<%- include("includes/footer.ejs") %>
